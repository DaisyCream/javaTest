package test02;



public class Sort {
	public static void main(String[] args){
		
	}
	
	public static int sort(int[] array){
		int l = 1;
		int r = array.length-1;
		int tmp = array[0];
		while(l<r){
			while(array[l]<=tmp&&l<r){
				l++;
			}
			while(array[r]>=tmp&&l<r){
				r--;
			}
			
		}
	}
}
